  "properties": {
    "CustomSlime": {
      "type": "object",
      "properties": {
        "SLIME_NAME": {
          "type": "string",
          "default": "Customizable Slime"
        },
        "TARR_SUPPORT": {
          "type": "boolean",
          "default": true
        },
        "CAN_LARGOFY": {
          "type": "boolean",
          "default": false
        },
        "WHAT_SLIME_LOOKS_LIKE": {
          "$ref": "#/definitions/Slimes",
          "default": "PINK_SLIME"
        },
        "WHAT_SLIME_ACTS_LIKE": {
          "$ref": "#/definitions/Slimes",
          "default": "PINK_SLIME"
        },
        "WHAT_SLIME_MATERIAL": {
          "$ref": "#/definitions/Slimes",
          "default": "PINK_SLIME"
        },
        "WHAT_SLIME_PRODUCES": {
          "type": "array",
          "items": {
            "anyOf": [
              { "$ref": "#/definitions/Plorts" },
              { "$ref": "#/definitions/Crafts" },
              { "$ref": "#/definitions/Crates" }
            ]
          },
          "default": ["PINK_SLIME"]
        },
        "WHAT_SLIME_EATS": {
          "type": "array",
          "items": {
            "anyOf": [{ "$ref": "#/definitions/Foods" }]
          },
          "default": ["VEGGIES"]
        },
        "ADDITIONAL_FOOD_SLIME_EATS": {
          "type": "array",
          "items": {
            "anyOf": [{ "$ref": "#/definitions/Foods" }]
          },
          "default": ["CARROT_VEGGIE"]
        },
        "FAVORITE_SLIME_EATS": {
          "type": "array",
          "items": {
            "anyOf": [
              {
                "$ref": "#/definitions/Foods"
              }
            ]
          },
          "default": ["HEART_BEET"]
        },
        "FAVORITE_SLIME_TOY": {
          "type": "array",
          "items": {
            "anyOf": [
              {
                "$ref": "#/definitions/Toys"
              }
            ]
          },
          "default": ["KOOKADOBA_BALL"]
        },
        "VACPACK_SIZE": {
          "enum": ["NORMAL", "LARGE", "GIANT"],
          "default": "NORMAL"
        },
        "SLIMEPEDIA_TITLE": {
          "type": "string",
          "default": "Customizable Slime"
        },
        "SLIMEPEDIA_INTRO": {
          "type": "string",
          "default": "This slime can be customized via configuration files!"
        },
        "SLIMEPEDIA_DIET": {
          "type": "string",
          "default": "Veggies"
        },
        "SLIMEPEDIA_FAVORITE": {
          "type": "string",
          "default": "Heart Beet"
        },
        "SLIMEPEDIA_SLIMEOLOGY": {
          "type": "string",
          "default": "[Insert Long Description Here] and that's what the Customizable Slime can do!"
        },
        "SLIMEPEDIA_RISKS": {
          "type": "string",
          "default": "Don't break your slime! Be a little bit careful when you configure it."
        },
        "SLIMEPEDIA_PLORTONOMICS": {
          "type": "string",
          "default": "Customizable Plorts are worth a lot of money!"
        },
        "SLIME_TOP_COLOR_RGB": {
          "type": "string",
          "pattern": "^\\s*\\d{1,3}\\s*,\\s*\\d{1,3}\\s*,\\s*\\d{1,3}\\s*$",
          "default": "255, 255, 255"
        },
        "SLIME_MIDDLE_COLOR_RGB": {
          "type": "string",
          "pattern": "^\\s*\\d{1,3}\\s*,\\s*\\d{1,3}\\s*,\\s*\\d{1,3}\\s*$",
          "default": "255, 255, 255"
        },
        "SLIME_BOTTOM_COLOR_RGB": {
          "type": "string",
          "pattern": "^\\s*\\d{1,3}\\s*,\\s*\\d{1,3}\\s*,\\s*\\d{1,3}\\s*$",
          "default": "255, 255, 255"
        },
        "SLIME_SPEC_COLOR_RGB": {
